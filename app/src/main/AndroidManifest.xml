<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abyandafa.playobook">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity" android:theme="@style/RegisterTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".main.MainActivity"
            android:label="@string/title_activity_main" />
        <activity android:name=".main.fragments.event.available.detail.AvailableDetailActivity" />
        <activity android:name=".main.fragments.event.play_on.detail.PlayOnDetailActivity" />
        <activity android:name=".RegisterActivity" android:theme="@style/RegisterTheme"></activity>
    </application>

</manifest>